@{
    ViewData["Title"] = "Грешка";
}

<div class="bg-danger p-4 text-white rounded-lg">
    <h3>@TempData["ErrorMessage"]</h3>
    <a onclick="goBack()" class="btn btn-warning"><b>Назад</b></a>
</div>

@section Scripts {
    <script>
        function goBack() {
            window.history.back();
        }
    </script>
}
